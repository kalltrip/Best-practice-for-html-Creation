   (function (w, d, s, l, i) {
    w[l] = w[l] || []; // Initialize the dataLayer array if it's not already defined
    w[l].push({
        'gtm.start': new Date().getTime(), // Record the start time of GTM
        event: 'gtm.js' // Push an event to the dataLayer
    });
    
    var f = d.getElementsByTagName(s)[0], // Get the first script element in the document
        j = d.createElement(s), // Create a new script element
        dl = l != 'dataLayer' ? '&l=' + l : ''; // If the dataLayer isn't 'dataLayer', append it to the URL
    
    j.async = true; // Make the script load asynchronously
    j.src = 'https://www.googletagmanager.com/gtm.js?id=' + i + dl; // Set the source URL for the GTM script
    
    f.parentNode.insertBefore(j, f); // Insert the new script element before the first existing script element
})(window, document, 'script', 'dataLayer', 'GTM-MN93XHD');

    <!-- End Google Tag Manager -->
